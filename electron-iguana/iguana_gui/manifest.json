{
  "name": "iguana",
  "short_name": "iguana",
    "version": "44.1.2",
    "manifest_version": 2,
    "description": "iguana",
    "offline_enabled": true,
  "description": "A secure Bitcoin wallet for everyone",
  "version": "1.7.1",
 "minimum_chrome_version": "36",
  "sockets": {
    "tcp": {
      "connect": "*"
    },
    "tcpServer": {
      "listen": "*"
    }
  },
  "permissions": [ 
      "unlimitedStorage", 
      {"fileSystem": ["write"]},
      "storage",
      "system.storage",
      "system.display",
      "system.network",
      "system.cpu",
      "notifications",
    "videoCapture",
    "http://127.0.0.1:14632/",
    "http://127.0.0.1:8332/"
    ]
,
  "app": {
    "background": {
      "scripts": ["initial.js"]
    }
  },
  "icons": {
    "128": "img/icons/icon128.png"
  }
}